function keyScheduleToHexStr(keySchedule) {  // return expanded key as hex words, as per FIPS-197§A
  var d = '';
  for (w=0; w<keySchedule.length; w++) {
    for (var b=0; b<4; b++) {
      var byte = keySchedule[w][b];
      d += byte<0x10 ? '0'+byte.toString(16) : byte.toString(16);
    }
    d += ' ';
  }
  return d;
}

